{
  "name": "discord-bot-template",
  "version": "1.1.2",
  "description": "A template for discord bots in typescript.",
  "main": "build/index.js",
  "scripts": {
    "start": "node .",
    "prod": "NODE_ENV=\"production\" yarn start",
    "cs": "yarn build && yarn start",
    "build": "yarn run clean && yarn run build:ts",
    "build:ts": "tsc -p ./",
    "test": "yarn run lint src/",
    "lint": "eslint --cache --report-unused-disable-directives --ext .js,.ts --ignore-path .gitignore",
    "lint:fix": "yarn run lint src/ --fix",
    "clean": "rimraf ./build && mkdir build",
    "preversion": "yarn test",
    "postversion": "git push && git push --tags",
    "postinstall": "husky install",
    "prettiest": "prettier --write . && yarn run lint:fix"
  },
  "dependencies": {
    "@tmuniversal/counter": "^1.0.5",
    "@tmware/status-rotate": "^1.1.1",
    "app-root-path": "^3.0.0",
    "axios": "0.21.1",
    "countapi-js": "^1.0.2",
    "discord-akairo": "^8.1.0",
    "discord.js": "12.5.1",
    "moment": "^2.29.1",
    "moment-duration-format": "^2.3.2"
  },
  "devDependencies": {
    "@tmuniversal/ts": "^1.1.2",
    "@tmware/eslint-config": "^1.1.1",
    "@tmware/eslint-config-typescript": "^1.0.5",
    "@tmware/prettier-config": "^1.0.2",
    "@types/app-root-path": "^1.2.4",
    "@types/debug": "^4.1.5",
    "@types/moment-duration-format": "^2.2.2",
    "@types/node": "^14.14.26",
    "@types/ws": "^7.4.0",
    "eslint": "^7.19.0",
    "husky": "^5.0.8",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TMUniversal/discord-bot-template.git"
  },
  "keywords": [
    "discord",
    "discord.js",
    "bot",
    "discord",
    "bot"
  ],
  "author": "TMUniversal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TMUniversal/discord-bot-template/issues"
  },
  "homepage": "https://github.com/TMUniversal/discord-bot-template#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "prettier": "@tmware/prettier-config"
}
