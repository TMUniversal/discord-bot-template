{
  "name": "discord-bot-template",
  "version": "0.1.3",
  "description": "A template for discord bots in typescript.",
  "main": "build/index.js",
  "scripts": {
    "start": "node .",
    "cs": "npm run build && node .",
    "build": "npm run clean && npm run build:ts",
    "build:ts": "tsc -p ./",
    "test": "npm run lint",
    "startmon": "ts-node-dev --respawn --transpileOnly ./src/index.ts",
    "lint": "eslint --ext .js,.ts --ignore-path .gitignore src/",
    "lint:fix": "npm run lint --fix",
    "clean": "rimraf ./build && mkdir build",
    "preversion": "npm run test",
    "postversion": "git push && git push --tags",
    "commit": "npx git-cz"
  },
  "dependencies": {
    "@tmuniversal/counter": "^1.0.2",
    "@tmuniversal/ts": "^1.0.3",
    "@tmware/jitsuyo": "^1.1.1",
    "@tmware/variable-parser": "^1.1.1",
    "app-root-path": "^3.0.0",
    "axios": "^0.19.2",
    "countapi-js": "^1.0.1",
    "discord-akairo": "^8.1.0",
    "discord.js": "12.2.0",
    "moment": "^2.24.0",
    "moment-duration-format": "^2.3.2"
  },
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/debug": "^4.1.5",
    "@types/lodash": "^4.14.161",
    "@types/moment-duration-format": "^2.2.2",
    "@types/node": "^14.0.24",
    "@types/ws": "^7.2.6",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.7.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.0.0-pre.61",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TMUniversal/discord-bot-template.git"
  },
  "keywords": [
    "discord",
    "discord.js",
    "bot",
    "discord",
    "bot"
  ],
  "author": "TMUniversal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TMUniversal/discord-bot-template/issues"
  },
  "homepage": "https://github.com/TMUniversal/discord-bot-template#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:fix && npm run lint",
      "pre-push": "npm run test"
    }
  }
}
